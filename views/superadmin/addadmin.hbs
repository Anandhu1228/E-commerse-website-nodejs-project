<section>
    <div class="container mt-4">
        <div class="row">
            <div class="col-md-6">
                <h2 class="text-center">Add Admin</h2>
                <form action="addadmin" method="POST" enctype="multipart/form-data">
                    <label for="">Name</label>
                    <input type="text" name="Name" class="form-control">

                    <label for="">Email</label>
                    <input type="email" name="Email" class="form-control">

                    <label for="">Contact No</label>
                    <input type="text" name="Contact" class="form-control">

                    <label for="">Passkey</label>
                    <input type="text" name="Passkey" class="form-control">

                    <label for="">Confirm Passkey</label>
                    <input type="text" name="Cpasskey" class="form-control">

                    
                    <label for="">Uniquekey</label>
                    <input type="text" name="Ukey" class="form-control">

                    <label for="">Image</label>
                    <input type="file" name="Image" class="form-control">

                    <button type="submit" class="btn btn-warning mt-4">Submit</button>
                    <button type="reset" class="btn btn-warning mt-4">Reset</button>
                    <a href="/superadmin/viewadmin" class="btn btn-danger mt-4">Cancel</a>
                </form>
            </div>
        </div>
    </div>
</section>